    "highlight.js": "^11.10.0",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.1",
    "nodemon": "^3.1.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.8.0",
